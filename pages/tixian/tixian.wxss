page{
  background: #f5f5f5;
}
.container{
  width:100%;
  height:100%;
  box-sizing: border-box;
  padding:0 20rpx;
}
.content{
  width:90%;
  height:400rpx;
  background:#fff;
  margin-top:110rpx;
  box-sizing:border-box;
  padding: 105rpx 98rpx 0 50rpx;
  box-shadow: 0 5rpx 6rpx #ddd;
}
.tixian{
  font-size:32rpx;
  color:#3e4452;
  margin-bottom:70rpx;
  font-weight:900;
}
.tixianMoney{
  width:100%;
  height:76rpx;
  display: flex;
}
.tixianMoney text{
  display:block;
  width:70rpx;
  font-size:50rpx;
}
.money{
  flex:1;
  border-bottom:1px solid #dedede;
}

button{
  width:90%;
  height:80rpx;
  color:#fff;
  line-height:80rpx;
  background: #ff6490;
  margin-top:100rpx;
}

.btn{
  background:#f695b0;
  color:#f5f5f5
}